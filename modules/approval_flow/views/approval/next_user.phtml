<?php
$total_count = $users->total_count();
if (!$total_count) :?>
<a class="prevent_default"><?php echo H('未知用户'); ?></a>
<?php elseif ($total_count):?>
    <?php foreach($users as $user):?>
        <a style="text-decoration: underline;" href="<?php echo H($user->url()) ?>" class="prevent_default"><?php echo H($user->name); ?></a>
    <?php endforeach;?>
<?php endif;?>
